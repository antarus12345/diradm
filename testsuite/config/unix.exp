# vim: ts=4 sw=4:

load_lib "testsuite/config-slapd/slapd.exp"


proc diradm_start {} {
	send_user "diradm_start\n"
	slapd_start
	ldapinit
}

proc diradm_exit {} {
	send_user "diradm_exit\n"
	slapd_exit
}
proc diradm_version {} {
	send_user "diradm version\n"
}

# Start the program running we want to test
diradm_start
