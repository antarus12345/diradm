#!/bin/sh
rm -f config.cache
echo "- aclocal."
aclocal-1.9 -I m4
echo "- autoconf."
autoconf-2.59
echo "- autoheader."
autoheader-2.59 --warnings all
echo "- automake."
automake-1.9 --add-missing --copy
exit
